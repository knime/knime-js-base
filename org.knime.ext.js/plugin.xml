<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="MoleculeSketcher" name="MoleculeSketcher"
         schema="schema/MoleculeSketcher.exsd"/>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.knime.ext.js.molecule.MoleculeSketcherPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.knime.workbench.ui.preferences"
            class="org.knime.ext.js.molecule.MoleculeSketcherPreferencePage"
            id="org.knime.ext.js.node.molecule"
            name="Molecule Sketcher">
      </page>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/flowabstraction/quickforms/input"
            factory-class="org.knime.ext.js.node.quickform.input.molecule.MoleculeStringInputQuickFormNodeFactory">
      </node>
      <node
            category-path="/flowabstraction/widgets/input"
            factory-class="org.knime.ext.js.node.widget.input.molecule.MoleculeWidgetNodeFactory">
      </node>
      <node
            category-path="/flowabstraction/widgets/input"
            factory-class="org.knime.ext.js.node.widget.input.molecule2.MoleculeWidgetNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.js.core.webResources">
      <webResourceBundle
            debug="true"
            name="Ketcher Molecule Sketcher"
            usesDefine="false"
            version="1.1 Beta"
            webResourceBundleID="Ketcher_1.1">
         <webResource
               relativePathSource="js-lib/ketcher/"
               relativePathTarget="js-lib/ketcher/">
         </webResource>
         <importResource
               relativePath="js-lib/ketcher/prototype-min.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/raphael.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/base64.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/third_party/keymaster.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/util/common.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/util/vec2.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/util/set.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/util/map.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/util/pool.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/element.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/struct.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/molfile.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/sgroup.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/struct_valence.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/dfs.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/cis_trans.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/stereocenters.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/smiles.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/chem/inchi.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/visel.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/restruct.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/restruct_rendering.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/render.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/templates.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/editor.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/elem_table.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/rnd/rgroup_table.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/ui/ui.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/ui/actions.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/reaxys/reaxys.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/ketcher.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/ketcher.css"
               type="CSS">
         </importResource>
      </webResourceBundle>
      <webResourceBundle
            debug="true"
            name="Molecule Input Quickform"
            version="1.0.0"
            webResourceBundleID="org.knime.ext.js.node.quickform.input.molecule.bundle">
         <webResource
               relativePathSource="js-src/org/knime/ext/js/node/quickform/input/molecule/"
               relativePathTarget="org/knime/ext/js/node/quickform/input/molecule/">
         </webResource>
         <webResource
               relativePathSource="js-lib/ketcher/"
               relativePathTarget="js-lib/ketcher/">
         </webResource>
         <importResource
               relativePath="org/knime/ext/js/node/quickform/input/molecule/MoleculeStringInput.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher/ketcher.css"
               type="CSS">
         </importResource>
         <webDependency
               webResourceBundleID="KnimeQuickformUtils_1.0.0">
         </webDependency>
         <webDependency
               webResourceBundleID="RequireJS_2.3.3">
         </webDependency>
      </webResourceBundle>
      <webResourceBundle
            debug="true"
            name="Molecule Widget"
            version="1.0.0"
            webResourceBundleID="org.knime.ext.js.node.widget.input.molecule.bundle">
         <webResource
               relativePathSource="js-src/org/knime/ext/js/node/widget/input/molecule/"
               relativePathTarget="org/knime/ext/js/node/widget/input/molecule/">
         </webResource>
         <importResource
               relativePath="org/knime/ext/js/node/widget/input/molecule/MoleculeWidget.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher2.0/ketcher.css"
               type="CSS">
         </importResource>
         <importResource
               relativePath="org/knime/ext/js/node/widget/input/molecule/MoleculeWidget.css"
               type="CSS">
         </importResource>
         <webDependency
               webResourceBundleID="KnimeQuickformUtils_1.0.0">
         </webDependency>
         <webDependency
               webResourceBundleID="RequireJS_2.3.3">
         </webDependency>
      </webResourceBundle>
      <webResourceBundle
            debug="false"
            name="Ketcher Molecule Sketcher"
            usesDefine="false"
            version="2.0"
            webResourceBundleID="Ketcher_2.0">
         <webResource
               relativePathSource="js-lib/ketcher2.0/"
               relativePathTarget="js-lib/ketcher2.0/">
         </webResource>
         <webResource
               relativePathSource="js-lib/ketcher2.0/doc"
               relativePathTarget="doc">
         </webResource>
         <webResource
               relativePathSource="js-lib/ketcher2.0/logo"
               relativePathTarget="logo">
         </webResource>
         <importResource
               relativePath="js-lib/ketcher2.0/raphael.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="js-lib/ketcher2.0/ketcher.css"
               type="CSS">
         </importResource>
      </webResourceBundle>
   </extension>
   <extension
         point="org.knime.js.core.javascriptComponents">
      <javascriptComponent
            getComponentValue-method-name="value"
            implementationBundleID="org.knime.ext.js.node.quickform.input.molecule.bundle"
            init-method-name="init"
            javascriptComponentID="org.knime.ext.js.node.quickform.input.molecule"
            name="Molecule Input Quickform"
            namespace="org_knime_ext_js_node_quickform_input_molecule"
            setValidationError-method-name="setValidationErrorMessage"
            validate-method-name="validate">
      </javascriptComponent>
      <javascriptComponent
            getComponentValue-method-name="value"
            implementationBundleID="org.knime.ext.js.node.widget.input.molecule.bundle"
            init-method-name="init"
            javascriptComponentID="org.knime.ext.js.node.widget.input.molecule"
            name="Molecule Widget"
            namespace="knimeMoleculeWidget"
            setValidationError-method-name="setValidationErrorMessage"
            validate-method-name="validate">
      </javascriptComponent>
   </extension>

</plugin>
